<% if(errors.length> 0) { %>
            <div class="bg-red-100 border border-red-400 px-4 py-3 rounded relative mb-4" role="alert">
              <strong class="font-bold text-red-700">Error!</strong>
              <ul class="mt-2 list-disc list-inside text-red-700">
                <% errors.forEach(error=> { %>
                  <li>
                    <%= error %>
                  </li>
                  <% }) %>
              </ul>
            </div>
            <% } %>